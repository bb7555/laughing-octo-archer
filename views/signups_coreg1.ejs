<script type="text/javascript">
var valMsg = "";


var validate = function(username, currentPass, email)
{
  if(username.length < 3 || username.length > 33){ valMsg = "Please choose a username between 3-32 characters"; return valMsg;}
  if(currentPass.length < 6){valMsg = "Please choose a password with at least 6 characters"; return valMsg;}
  if(email.length < 3){valMsg = "Please provide a valid email"; return valMsg;}
  valMsg = "";
  return valMsg;
}

var failValidate = function(valMsg)
{
  
  $("#status").appendTo(valMsg);
  $("#overlay").hide();
  return valMsg;

}

$(document).ready(function(){
	$('input[name="password"]').keyup(function(){
			currentPass = $(this).val();
			$('input[name="vpassword"]').val(currentPass);
			});
	$('form').submit( function(ev) {
		$("#overlay").fadeIn();
     ev.preventDefault();
    var username = $('input[name="username"]').val() 
    , currentPass = $('input[name="vpassword"]').val()
	  , first_name = $('input[name="first_name"]').val()
	  , middle_initial = $('input[name="middle_initial"]').val()
	  , last_name = $('input[name="last_name"]').val()
	  , email  = $('input[name="email"]').val()
	  , phone= $('input[name="phone"]').val()
	  , address= $('input[name="address"]').val()
	  , city= $('input[name="city"]').val()
	  , state = $('select[name="state"]').val()
		, zip = $('input[name="zip"]').val()
		, month  = $('select[name="month"]').val()
		, year  = $('select[name="year"]').val()
		, gender = $('select[name="gender"]').val();

			validate(username, currentPass, email);
      
if(valMsg === ""){
			
	$.get("http://www.getthatjob.info/signups/saveAsync?first_name="+first_name+"&middle_initial="+middle_initial+"&last_name="+last_name+"&email="+email+"&phone="+phone+"&address="+address+"&city="+city+"&state="+state+"&zip="+zip+"&month="+month+"&year="+year+"&gender="+gender+"&username="+username+"", 
	function(data){
  setTimeout(function(){$('form').unbind('submit').submit();},3000);
		
		});

    }else{

    alert(valMsg);
    failValidate(valMsg);
    }
	 
   
});
	
	});
</script>



